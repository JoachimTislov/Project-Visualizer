        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src/main.go",
            "args": [
                "-add",
                "-m",
                "Test",
                "-content",
                "/home/yoa/RefViz/sample-code/quickfeed/internal/env/path.go",
                "-n",
                "main"
            ]
        },
        {
            "name": "Launch Graphviz",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src/main.go",
            "args": [
                "-graphviz",
                "Peter"
            ]
        }
    ]
